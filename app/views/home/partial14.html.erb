<div class=" col-partial4">
  <h4 id ="newsletterMessage">{{viewMessageFourteen}}</h4>
  <form ng-submit = submit() name ="frm" >
  <div class="row" ng-cloak><!--Get Longitude/Latitude -->
    <div class="small-12 columns">
      <dl class="sub-nav">
        <dd>
            <input class ="button tiny"
                   ng-disabled = "frm.$invalid"
                   id ="btnNewsletter"
                   type ="submit"
                   value ="subscribe"/>
        </dd>
        <dd>
          <label for="email">Email</label>
        </dd>
        <dd>
          <input type="email" name = "email" required ng-model ="email" ng-focus="onFocus()"
                 ng-blur="onBlur()"/>
        </dd>
        <dd>
          <!--<span class ="newsLetterErr" ng-show ="frm.email.$dirty && frm.email.$error.required">Required</span> -->
          <span ng-class = " errorMessage ?  'opacityZero': ''" class ="newsLetterErr" ng-show ="frm.email.$dirty && frm.email.$error.email">Not a valid email</span>
        </dd>
        <dd></dd>
        <dd ng-show ="thankYouMessage" >Thank you, &nbsp;{{userName | uppercase}},&nbsp;you have subscribed to our news letter <br/>(email address: {{userEmail}})</dd>
        <dd ng-show ="thankYouMessage" >{{message}}</dd>
      </dl>
    </div>
  </div>
  </form>
  <hr/>
  <h3>Search</h3>
  <div class="searchBox"><input ng-model="search"/></div>
  <div class="table_lge_wrapper">
    <table>
      <tr ng-repeat="user in users | orderBy: 'lname'| filter: search">
        <td>{{user.lname | uppercase}}</td>
        <td>{{user.fname | lowercase}}</td>
        <td>{{user.tel}}</td>
        <td>{{user.id}}</td>
        <td>{{user.ip}}</td>
      </tr>
    </table>
  </div>
</div>



